#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int n,k;
	    cin>>n>>k;
	    int g;
	    int s=0;
	    
	    int a[n];
	    int m=-1;
	    for(int i=0;i<n;i++)
	    {
	        cin>>a[i];
	    }
	    
	    for(int i=0;i<=n-k;i++){
            s=0;
         for(int j=i;j<i+k;j++){
                s=s+a[j];
            }
            m=max(m,s);
        }
        cout<<m<<endl;
	    
	 
	    
	    
	}
	return 0;
}
